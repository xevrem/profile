{
  "name": "profile",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://xevrem.github.io/profile",
  "dependencies": {
    "prop-types": "^15",
    "react": "^16",
    "react-dom": "^16",
    "react-modal": "^3"
  },
  "scripts": {
    "start": "yarn gulp setup && yarn parcel ./src/index.html",
    "build": "yarn gulp setup && yarn parcel build src/index.html --public-url /profile --dist-dir ./dist/profile",
    "test": "jest",
    "setup": "yarn gulp setup",
    "predeploy": "yarn setup",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.15.8",
    "@babel/eslint-plugin": "^7.14.5",
    "@babel/preset-env": "^7.15.8",
    "@parcel/packager-raw-url": "^2.0.0",
    "@parcel/transformer-sass": "^2.0.0",
    "@parcel/transformer-webmanifest": "^2.0.0",
    "del": "^6.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.0.1",
    "eslint-plugin-react": "^7.10.0",
    "gh-pages": "^3",
    "gulp": "^4.0.2",
    "gulp-run": "^1.7.1",
    "jest": "^23.4.1",
    "parcel": "^2",
    "prettier": "^2.4.1"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/style_mock.js"
    }
  },
  "packageManager": "yarn@3.0.2"
}
